<h1>New evenement</h1>

<% form_for(@evenement) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :nom %><br />
    <%= f.text_field :nom %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description %>
  </p>
  <p>
    <%= f.label :affichage %><br />
    <%= f.check_box :affichage %>
  </p>
  <p>
    <%= f.label :portee %><br />
    <%= f.text_field :portee %>
  </p>
  <p>
    <%= f.label :debut %><br />
    <%= f.datetime_select :debut %>
  </p>
  <p>
    <%= f.label :duree %><br />
    <%= f.text_field :duree %>
  </p>
  <p>
    <%= f.label :campu_id %><br />
    <%= f.text_field :campu_id %>
  </p>
  <p>
    <%= f.label :espace_id %><br />
    <%= f.text_field :espace_id %>
  </p>
  <p>
    <%= f.label :intervenant_id %><br />
    <%= f.text_field :intervenant_id %>
  </p>
  <p>
    <%= f.label :cour_id %><br />
    <%= f.text_field :cour_id %>
  </p>
  <p>
    <%= f.label :evaluation_id %><br />
    <%= f.text_field :evaluation_id %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', evenements_path %>
